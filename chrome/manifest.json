{
  "name": "Hyperliquid.xyz Access Extension",
  "version": "1.0",
  "description": "Helps access Hyperliquid when you are on international airplane WiFi.",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://app.hyperliquid.xyz//*",
    "https://api-ui.hyperliquid.xyz/info*"
  ],
  "background": {
    "scripts": [
      "hyperliquid.js"
    ],
    "persistent": true
  }
}